{
  "name": "project-begin",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "cp_public": "mkdir -p dist && cp -r public/* dist",
    "build": "npm run cp_public && cross-env NODE_ENV=production webpack build --config ./webpack/webpack.prod.js",
    "start": "npm run cp_public && webpack serve --hot --open --config ./webpack/webpack.dev.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "check-type": "tsc --watch"
  },
  "keywords": [],
  "author": "taoyingsong",
  "license": "MIT",
  "sideEffect": [
    "*.{c,le,sa,sc}ss"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{tsx,ts,jsx,js,json,css,less,scss,md}": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@reduxjs/toolkit": "^1.7.2",
    "@types/lodash": "^4.14.178",
    "antd": "^4.18.5",
    "axios": "^0.26.0",
    "classnames": "^2.3.1",
    "date-fns": "^2.28.0",
    "faker": "^5.5.3",
    "lodash": "^4.17.21",
    "miragejs": "^0.1.43",
    "mock-socket": "^9.1.2",
    "msw": "^0.36.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.1",
    "reselect": "^4.1.5",
    "seedrandom": "^3.0.5",
    "txtgen": "2.2.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.12",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@mswjs/data": "^0.8.4",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.4",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-redux": "^7.1.22",
    "@types/webpack-env": "^1.16.3",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "autoprefixer": "^10.4.2",
    "babel-jest": "^27.4.0",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.4.0",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "lint-staged": "^12.3.3",
    "mini-css-extract-plugin": "^2.5.3",
    "postcss": "^8.4.6",
    "postcss-loader": "^6.2.1",
    "postcss-nested": "^5.0.6",
    "prettier": "2.5.1",
    "react-refresh": "^0.11.0",
    "redux-devtools": "^3.7.0",
    "regenerator-runtime": "^0.13.9",
    "serve": "^12.0.0",
    "speed-measure-webpack-plugin": "^1.5.0",
    "style-loader": "^3.3.1",
    "thread-loader": "^3.0.4",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.6",
    "type-fest": "^2.11.1",
    "typescript": "^4.5.5",
    "webpack": "^5.38.1",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^4.7.3",
    "webpack-merge": "^5.8.0"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/jest-setup.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$",
      "^.+\\.module\\.(css|less)$"
    ],
    "moduleNameMapper": {
      "^.+.(css|less)$": "identity-obj-proxy",
      "^.+\\.module\\.(css|less)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|svg)$": "<rootDir>/src/__mocks__/fileMock.ts"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  }
}
